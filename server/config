export home=.
export certs_dir="$home/certs"

export ca_home="$home/ca"
export database="$ca_home/index.txt"
export serial="$ca_home/serial"


export ca_key="$ca_home/ca.key"
export ca_crt="$ca_home/ca.crt"

export server_key="$home/server.key"
export server_crt="$home/server.crt"


export ca_commonName="OpenVPN CA"
export server_commonName="OpenVPN Server"
